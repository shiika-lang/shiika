# Class of metaclasses
# see also: src/corelib/
class Metaclass : Class
  def initialize(@name: String, @base_name: String)
  end

  def name -> String
    if @base_name == "Metaclass"
      "Metaclass"
    else
      "Meta:" + @base_name
    end
  end

  def inspect -> String
    "#<metaclass " + self.name + ">"
  end
end
